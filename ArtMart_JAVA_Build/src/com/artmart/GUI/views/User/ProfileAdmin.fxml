<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.User.ProfileAdminController">
   <children>
      <ImageView fx:id="ProfilePic" fitHeight="150.0" fitWidth="113.0" layoutX="60.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../assets/user.jpeg" />
         </image>
      </ImageView>
      <Label fx:id="usernameProfile" layoutX="180.0" layoutY="52.0" text="Label" />
      <Label fx:id="nameProfile" layoutX="176.0" layoutY="224.0" prefHeight="17.0" prefWidth="65.0" text="Label" />
      <Label layoutX="71.0" layoutY="261.0" text="Email:" />
      <Label fx:id="emailProfile" layoutX="177.0" layoutY="261.0" text="Label" />
      <Label layoutX="471.0" layoutY="52.0" text="Department" />
      <Label fx:id="departmentProfile" layoutX="478.0" layoutY="80.0" text="Label">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label layoutX="69.0" layoutY="295.0" text="BirthDate:" />
      <Label fx:id="birthdayProfile" layoutX="177.0" layoutY="295.0" text="Label" />
      <Button fx:id="editProfileBtn" layoutX="520.0" layoutY="365.0" mnemonicParsing="false" onAction="#OnUpdateBtn" text="Edit Profile" />
      <Button fx:id="backBtn" layoutX="16.0" layoutY="20.0" mnemonicParsing="false" onAction="#OnBack" text="&lt;Back" />
      <Label layoutX="69.0" layoutY="328.0" text="Phone Number:" />
      <Label fx:id="phoneProfile" layoutX="177.0" layoutY="328.0" text="Label" />
      <Button fx:id="userListBtn" layoutX="405.0" layoutY="229.0" mnemonicParsing="false" onAction="#OnUserListBtn" text="View Users List " />
      <Button fx:id="addAdBtn" layoutX="416.0" layoutY="266.0" mnemonicParsing="false" onAction="#OnAddBtn" text="Add Admin" />
   </children>
</AnchorPane>
