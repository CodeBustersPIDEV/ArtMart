<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.User.ProfileArtistController">
   <children>
      <ImageView fx:id="ProfilePic" fitHeight="150.0" fitWidth="113.0" layoutX="50.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../assets/user.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="170.0" layoutY="65.0" text="Bio" textFill="#373065">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label layoutX="59.0" layoutY="214.0" text="Name:" textFill="#373065">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="usernameProfile" layoutX="170.0" layoutY="42.0" text="Label" textFill="#373065">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Label>
      <Label fx:id="nameProfile" layoutX="166.0" layoutY="214.0" prefHeight="17.0" prefWidth="65.0" text="Label" textFill="#373065" />
      <Label layoutX="61.0" layoutY="251.0" text="Email:" textFill="#373065">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="emailProfile" layoutX="167.0" layoutY="251.0" text="Label" textFill="#373065" />
      <Label layoutX="398.0" layoutY="33.0" text="Number of artworks posted:" textFill="#373065">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="nbrArtProfile" layoutX="468.0" layoutY="67.0" text="Label" textFill="#373065">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="bioProfile" layoutX="170.0" layoutY="105.0" prefHeight="64.0" prefWidth="236.0" text="Label" textFill="#373065" />
      <Label layoutX="59.0" layoutY="285.0" text="BirthDate:" textFill="#373065">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="birthdayProfile" layoutX="167.0" layoutY="285.0" text="Label" textFill="#373065" />
      <Button fx:id="editProfileBtn" layoutX="510.0" layoutY="355.0" mnemonicParsing="false" onAction="#OnUpdateBtn" style="-fx-background-color: #BAB9CD;" text="Edit Profile" />
      <Button fx:id="backBtn" layoutX="6.0" layoutY="10.0" mnemonicParsing="false" onAction="#OnBack" style="-fx-background-color: #BAB9CD;" text="&lt;Back" />
      <Label layoutX="-74.0" layoutY="77.0" text="Label" />
      <Label layoutX="59.0" layoutY="318.0" text="Phone Number:" textFill="#373065">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="phoneProfile" layoutX="167.0" layoutY="318.0" text="Label" textFill="#373065" />
      <Button fx:id="GenerateQrCode" layoutX="398.0" layoutY="355.0" mnemonicParsing="false" onAction="#OnQrCode" style="-fx-background-color: #BAB9CD;" text="GenerateQrCode" />
      <ImageView fx:id="QrCode" fitHeight="135.0" fitWidth="145.0" layoutX="419.0" layoutY="208.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
