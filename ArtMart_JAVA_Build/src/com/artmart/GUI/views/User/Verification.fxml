<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="256.0" prefWidth="427.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.User.VerificationController">
   <children>
      <TextField fx:id="verificationField" layoutX="139.0" layoutY="142.0" />
      <Text fill="#e1704b" layoutX="25.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please copy your token here to activate your account ">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Text>
      <Button fx:id="submitBtn" layoutX="188.0" layoutY="176.0" mnemonicParsing="false" onAction="#OnSubmit" style="-fx-background-color: #BAB9CD;" text="Submit" />
      <Label fx:id="msg" layoutX="200.0" layoutY="234.0" textFill="#dd0909" />
      <Hyperlink fx:id="resendLink" layoutX="249.0" layoutY="233.0" onAction="#OnResend" text="Resend Email" />
      <Text layoutX="117.0" layoutY="249.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Didn't receive any mail ?" />
      <Button fx:id="backBtn" layoutX="6.0" layoutY="6.0" mnemonicParsing="false" onAction="#OnBack" text="&lt;Back" />
   </children>
</AnchorPane>
