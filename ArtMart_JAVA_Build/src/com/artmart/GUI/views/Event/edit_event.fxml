<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="640.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Event.Edit_eventController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/event/edit_event.css" />
    </stylesheets>
   <children>
      <Label alignment="CENTER" layoutX="24.0" layoutY="38.0" prefHeight="65.0" prefWidth="138.0" text="Edit Event" textFill="#3f68ff">
         <font>
            <Font name="Agency FB Bold" size="33.0" />
         </font>
      </Label>
      <TextField fx:id="txtEventLocation" layoutX="80.0" layoutY="190.0" prefHeight="25.0" prefWidth="156.0" promptText="Location" />
      <TextField fx:id="txtEventName" layoutX="79.0" layoutY="122.0" prefHeight="25.0" prefWidth="156.0" promptText="Name" />
      <ComboBox fx:id="comboBoxEventType" layoutX="79.0" layoutY="155.0" prefHeight="25.0" prefWidth="156.0" promptText="Type" />
      <TextField fx:id="txtEventCapacity" layoutX="80.0" layoutY="299.0" prefHeight="25.0" prefWidth="156.0" promptText="Capacity" />
      <TextField fx:id="txtEventEntryFee" layoutX="80.0" layoutY="337.0" prefHeight="25.0" prefWidth="156.0" promptText="Entry Fee" />
      <TextArea fx:id="txtAreaEventDescription" layoutX="277.0" layoutY="123.0" prefHeight="242.0" prefWidth="299.0" promptText="Description" />
      <Button fx:id="btnConfirm" layoutX="447.0" layoutY="390.0" mnemonicParsing="false" onAction="#onBtnConfirm" prefHeight="38.0" prefWidth="85.0" text="Confirm" textAlignment="CENTER" textFill="#0d9e31">
         <font>
            <Font name="Agency FB Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelEvent" layoutX="328.0" layoutY="390.0" mnemonicParsing="false" onAction="#onBtnCancelEvent" prefHeight="38.0" prefWidth="85.0" text="Cancel" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Agency FB Bold" size="20.0" />
         </font>
      </Button>
      <DatePicker fx:id="dpEventEndDate" layoutX="80.0" layoutY="262.0" prefHeight="25.0" prefWidth="156.0" promptText="End  Date" />
      <DatePicker fx:id="dpEventStartDate" layoutX="80.0" layoutY="226.0" prefHeight="25.0" prefWidth="156.0" promptText="Start Date" />
   </children>
</AnchorPane>
