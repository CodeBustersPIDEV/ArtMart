<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.artmart.GUI.controllers.Event.User.Feedback.AddFeedbackController">
   <children>
      <Label alignment="CENTER" layoutX="54.0" layoutY="28.0" prefHeight="65.0" prefWidth="204.0" text="Add Feedback" textFill="#ff5946">
         <font>
            <Font name="Brush Script MT Italic" size="40.0" />
         </font>
      </Label>
      <Button layoutX="425.0" layoutY="333.0" mnemonicParsing="false" onAction="#onPost" prefHeight="27.0" prefWidth="64.0" text="Post" textAlignment="CENTER" textFill="#0fae22">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="506.0" layoutY="334.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="27.0" prefWidth="64.0" text="Cancel" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Text fill="#5d53a8" layoutX="60.0" layoutY="227.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Comment">
         <font>
            <Font name="Brush Script MT Italic" size="30.0" />
         </font>
      </Text>
      <Text fill="#5d53a8" layoutX="60.0" layoutY="168.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rating">
         <font>
            <Font name="Brush Script MT Italic" size="30.0" />
         </font>
      </Text>
      <TextArea fx:id="txtAreaComment" layoutX="185.0" layoutY="197.0" prefHeight="166.0" prefWidth="180.0" promptText="Share your thoughts about the event here." />
      <Spinner fx:id="spinnerRating" layoutX="185.0" layoutY="140.0" prefHeight="40.0" prefWidth="180.0" />
   </children>
</AnchorPane>
