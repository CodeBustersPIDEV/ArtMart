<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="650.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Event.Add_eventController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/event/add_event.css" />
    </stylesheets>
   <children>
      <Label alignment="CENTER" layoutX="24.0" layoutY="28.0" prefHeight="65.0" prefWidth="163.0" text="Add Event" textFill="#5d53a8">
         <font>
            <Font name="Brush Script MT Italic" size="40.0" />
         </font>
      </Label>
      <TextField fx:id="txtLocation" layoutX="70.0" layoutY="180.0" prefHeight="25.0" prefWidth="156.0" promptText="Location" />
      <TextField fx:id="txtName" layoutX="69.0" layoutY="112.0" prefHeight="25.0" prefWidth="156.0" promptText="Name" />
      <ComboBox fx:id="comboBoxType" layoutX="69.0" layoutY="145.0" prefHeight="25.0" prefWidth="156.0" promptText="Type" />
      <TextField fx:id="txtCapacity" layoutX="70.0" layoutY="289.0" prefHeight="25.0" prefWidth="156.0" promptText="Capacity" />
      <TextField fx:id="txtEntryFee" layoutX="70.0" layoutY="327.0" prefHeight="25.0" prefWidth="156.0" promptText="Entry Fee" />
      <TextArea fx:id="txtAreaDescription" layoutX="267.0" layoutY="113.0" prefHeight="242.0" prefWidth="341.0" promptText="Description" />
      <Button fx:id="btnAddEvent" layoutX="470.0" layoutY="381.0" mnemonicParsing="false" onAction="#onAddEvent" prefHeight="34.0" prefWidth="65.0" text="Confirm" textAlignment="CENTER" textFill="#0fae22">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font></Button>
      <Button fx:id="btnCancelEvent" layoutX="333.0" layoutY="381.0" mnemonicParsing="false" onAction="#returnToEventHomepage" prefHeight="34.0" prefWidth="65.0" text="Cancel" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font></Button>
      <DatePicker fx:id="dpEndDate" layoutX="70.0" layoutY="252.0" prefHeight="25.0" prefWidth="156.0" promptText="End  Date" />
      <DatePicker fx:id="dpStartDate" layoutX="70.0" layoutY="216.0" prefHeight="25.0" prefWidth="156.0" promptText="Start Date" />
   </children>
</AnchorPane>
