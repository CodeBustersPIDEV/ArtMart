<AnchorPane id="AnchorPane" prefHeight="710.0" prefWidth="654.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.artmart.GUI.controllers.Event.Artist.Participation.ListParticipationController">
   <children>
      <ScrollPane fx:id="scrollPanePar" hbarPolicy="NEVER" layoutY="162.0" prefHeight="564.0" prefWidth="654.0" vbarPolicy="ALWAYS">
