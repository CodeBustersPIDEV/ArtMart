            <VBox fx:id="vBox" prefHeight="570.0" prefWidth="640.0" />
         </content>
      </ScrollPane>
      <Text fill="#ff5946" layoutX="172.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Participations List">
