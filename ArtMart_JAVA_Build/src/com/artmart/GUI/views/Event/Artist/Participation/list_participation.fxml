            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnSearch" layoutX="231.0" layoutY="124.0" mnemonicParsing="false" onAction="#onBtnSearch" text="Search" textFill="#10a12f">
