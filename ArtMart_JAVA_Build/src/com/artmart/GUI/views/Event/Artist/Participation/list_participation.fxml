            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="txtSearch" layoutX="20.0" layoutY="124.0" onKeyPressed="#onTxtSearch" prefHeight="28.0" prefWidth="195.0" promptText="Type name of desired event here." />
