<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="650.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Event.Event_homeController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/event/event_home.css" />
    </stylesheets>
   <children>
      <Button fx:id="btnAddEvent" layoutX="135.0" layoutY="216.0" mnemonicParsing="false" onAction="#onBtnAddEvent" prefHeight="45.0" prefWidth="172.0" text="Add New Event" textFill="#2c8e9c">
         <font>
            <Font name="Agency FB Bold" size="24.0" />
         </font></Button>
      <Button id="btnReturn" layoutX="4.0" layoutY="3.0" mnemonicParsing="false" onAction="#returnToMain" text="&lt; Back" textFill="#5d53a8">
