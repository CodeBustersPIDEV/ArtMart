            <Font name="Agency FB Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="btnGetAllActivities" layoutX="340.0" layoutY="300.0" mnemonicParsing="false" onAction="#onBtnGetAllActivities" text="View Activity List" textFill="#2d489c">
         <font>
            <Font name="Agency FB Bold" size="24.0" />
         </font>
      </Button>
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</AnchorPane>
