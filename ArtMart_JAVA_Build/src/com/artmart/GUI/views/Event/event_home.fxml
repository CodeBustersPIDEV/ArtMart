      <Label layoutX="212.0" layoutY="56.0" text="Event Home Page">
         <font>
            <Font name="Agency FB Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="btnGetAllEvents" layoutX="334.0" layoutY="187.0" mnemonicParsing="false" onAction="#onGetAllEvents" text="View Event List">
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
