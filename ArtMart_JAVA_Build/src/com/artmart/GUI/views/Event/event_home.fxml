      <Button fx:id="btnGetAllEvents" layoutX="340.0" layoutY="216.0" mnemonicParsing="false" onAction="#onBtnGetAllEvents" prefHeight="45.0" prefWidth="172.0" text="View Event List" textFill="#2c8e9c">
