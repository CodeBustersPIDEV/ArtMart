<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Order.OrderManagmentController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/ordermanagment.css" />
    </stylesheets>
   <children>
      <Button fx:id="manageShippBtn" layoutX="468.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnClickShippment" text="Manage Shipping" />
      <TableView fx:id="orderTableView" layoutX="14.0" layoutY="67.0" prefHeight="317.0" prefWidth="564.0">
        <columns>
          <TableColumn fx:id="orderID_Col" prefWidth="54.0" text="Order_Id" />
          <TableColumn fx:id="user_Col" prefWidth="77.0" text="User" />
            <TableColumn fx:id="product_Col" prefWidth="87.0" text="Product" />
            <TableColumn fx:id="orderDate_Col" prefWidth="108.0" text="OrderDate" />
            <TableColumn fx:id="shippingAdd_Col" prefWidth="237.0" text="ShippingAddress" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnStatisticsClick" prefHeight="25.0" prefWidth="110.0" text="Performance" />
      <Button fx:id="managePayBtn" layoutX="342.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnClickPayment" text="Manage Payment" />
   </children>
</AnchorPane>
