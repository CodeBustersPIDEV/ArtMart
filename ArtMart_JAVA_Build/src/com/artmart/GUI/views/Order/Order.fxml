<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="649.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Order.OrderGUIController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/ordergui.css" />
    </stylesheets>
    <children>
      <Button fx:id="placeOrderBtn" layoutX="530.0" layoutY="393.0" mnemonicParsing="false" onAction="#OnOrderConfirm" prefHeight="25.0" prefWidth="79.0" text="Checkout" />
      <Button fx:id="cancelOrderBtn" layoutX="437.0" layoutY="393.0" mnemonicParsing="false" onAction="#OnOrderCancel" prefHeight="25.0" prefWidth="79.0" text="Cancel" />
      <TableView fx:id="productTableView" layoutX="14.0" layoutY="82.0" prefHeight="300.0" prefWidth="594.0">
        <columns>
          <TableColumn fx:id="name_col" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="desc_col" prefWidth="228.0" text="Description" />
            <TableColumn fx:id="dimension_col" minWidth="7.0" prefWidth="97.0" text="Dimensions" />
            <TableColumn fx:id="weight_col" prefWidth="100.0" text="Weight" />
            <TableColumn fx:id="mat_col" prefWidth="93.0" text="Material" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="14.0" layoutY="25.0" mnemonicParsing="false" onAction="#OnBackBtnl" prefHeight="25.0" prefWidth="142.0" text="Continue Shoping" />
      </children>
</AnchorPane>
