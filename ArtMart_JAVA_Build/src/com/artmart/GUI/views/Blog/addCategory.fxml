<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Blog.AddCategoryController">
   <children>
      <TextField fx:id="cat_nameTextField" layoutX="207.0" layoutY="185.0" promptText="Category Name" />
      <Label layoutX="172.0" layoutY="74.0" prefHeight="68.0" prefWidth="257.0" text="Add Category">
         <font>
            <Font name="Agency FB Bold" size="55.0" />
         </font>
      </Label>
      <Button fx:id="addCategory" layoutX="277.0" layoutY="255.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button fx:id="goBack" layoutX="26.0" layoutY="22.0" mnemonicParsing="false" onAction="#goBackToMenu" text="Cancel" />
   </children>
</AnchorPane>
