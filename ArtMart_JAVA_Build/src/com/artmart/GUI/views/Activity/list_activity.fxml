<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="705.0" prefWidth="650.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Activity.ListActivityController">
    <stylesheets>
        <URL value="@/com/artmart/GUI/styles/activity/list_activity.css" />
    </stylesheets>
   <children>
      <ScrollPane fx:id="scrollPaneActivityList" hbarPolicy="NEVER" layoutY="152.0" prefHeight="564.0" prefWidth="650.0" vbarPolicy="ALWAYS">
         <content>
            <VBox fx:id="vBox" prefHeight="570.0" prefWidth="635.0" />
         </content>
      </ScrollPane>
      <Text fill="#5d53a8" layoutX="229.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Activity List">
         <font>
            <Font name="Brush Script MT Italic" size="48.0" />
         </font>
      </Text>
      <Button layoutX="13.0" layoutY="13.0" mnemonicParsing="false" onAction="#onBtnReturn" text="&lt; Back">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnSearch" layoutX="380.0" layoutY="114.0" mnemonicParsing="false" onAction="#onBtnSearch" text="Search" textFill="#10a12f">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="txtSearch" layoutX="226.0" layoutY="114.0">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="456.0" layoutY="115.0" mnemonicParsing="false" onAction="#onBtnCancel" text="Cancel" textFill="RED">
         <font>
            <Font name="Agency FB Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
