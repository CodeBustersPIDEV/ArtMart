<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.artmart.GUI.controllers.Blog.EditBlogCategoryController">
   <children>
      <TextField fx:id="cat_nameTextField" layoutX="207.0" layoutY="185.0" promptText="Category Name" />
      <Label layoutX="172.0" layoutY="85.0" prefHeight="68.0" prefWidth="257.0" text="Edit Category">
         <font>
            <Font name="Agency FB Bold" size="55.0" />
         </font>
      </Label>
      <Button fx:id="editCategory" layoutX="276.0" layoutY="246.0" mnemonicParsing="false" onAction="#edit" text="Edit" />
      <Button fx:id="goBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBackToMenu" text="Cancel">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../../../assets/BlogAssets/back-arrow.png" />
               </source>
            </ImageInput>
         </effect></Button>
      <Label fx:id="catID" layoutX="548.0" layoutY="19.0" prefHeight="13.0" prefWidth="29.0" visible="false" />
   </children>
</AnchorPane>
